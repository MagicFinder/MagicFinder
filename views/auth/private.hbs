<div class="container-fluid">
  <div class="row">
    <div class="col-md-6">
      <h3>
        Welcome to your personal page {{allCards.username}}
      </h3>
      <p>
        Here you can edit your profile data, create you personal card collection
        <h5>Your Contact method</h5>
        <p>email: {{allCards.email}}</p>
      </p>
    </div>
    <div class="col-md-6">
      <img alt="profile image" src="{{allCards.imgPath}}">
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <h5>Your Cards</h5>
      

  {{#each allCards.cards}}
<div class="row">
<div class="col-md-6">


  <b>Card id:</b>
  <p>{{_id}}</p>
  <br>
  <b>Card Name:</b>
  <p>{{cardName}}</p>
  <br>
  <img alt="cardImage" src="{{cardImage}}">
  <br>
  <b>Card Description:</b>
  <p>{{cardDesc}}</p>
  <br>
  <b>Card Rarity:</b>
  <p>{{cardRarity}}</p>
  <br>
  <b>Card Price:</b>
  <p>{{cardPrice}}</p>
    </div>
    <div class="col-md-6"></div>
</div>
  {{/each}}




    </div>
  </div>
</div>











